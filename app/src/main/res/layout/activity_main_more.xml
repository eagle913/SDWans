<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txtReport" android:text="@string/main_more_report" android:drawablePadding="30dp" android:gravity="center_vertical" android:drawableLeft="@drawable/ic_launcher_background"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txtInfo" android:text="@string/main_more_info"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txtTraffic" android:text="@string/main_more_traffic"/>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txtConfig" android:text="@string/main_more_config"/>

</LinearLayout>
